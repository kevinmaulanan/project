apus!'); \/\/\u003C\/span\u003E\u003C\/div\u003E\u003Cdiv\u003E\u0026nbsp;\u0026nbsp;\u0026nbsp;\u0026nbsp;\u0026nbsp;\u0026nbsp;\u0026nbsp;\u0026nbsp;  \u003C\/div\u003E\u003Cdiv\u003E\u0026nbsp;\u0026nbsp;\u0026nbsp;\u0026nbsp;\u0026nbsp;\u0026nbsp;\u0026nbsp;\u0026nbsp;  \u003Cspan style=\"color: #4ec9b0;\"\u003Econsole\u003C\/span\u003E.\u003Cspan style=\"color: #dcdcaa;\"\u003Elog\u003C\/span\u003E(\u003Cspan style=\"color: #ce9178;\"\u003E\"berhasil remove\"\u003C\/span\u003E + \u003Cspan style=\"color: #9cdcfe;\"\u003Eresult\u003C\/span\u003E)\u003C\/div\u003E\u003Cdiv\u003E\u0026nbsp;\u0026nbsp;\u0026nbsp;\u0026nbsp;\u0026nbsp;\u0026nbsp;\u0026nbsp;\u0026nbsp;  \u003Cspan style=\"color: #9cdcfe;\"\u003Eres\u003C\/span\u003E.\u003Cspan style=\"color: #dcdcaa;\"\u003Ejson\u003C\/span\u003E(\u003Cspan style=\"color: #9cdcfe;\"\u003Eresult\u003C\/span\u003E) \u003Cspan style=\"color: #608b4e;\"\u003E\/\/hasil pembuangan di dikembalikan ke ajax\u003C\/span\u003E\u003C\/div\u003E\u003Cdiv\u003E\u0026nbsp;\u0026nbsp;\u0026nbsp;\u0026nbsp;  });});\u003C\/div\u003E\u003Cdiv\u003E\u0026nbsp;\u0026nbsp;\u0026nbsp;\u0026nbsp;  \u003C\/div\u003E\u003Cbr \/\u003E\u003Cbr \/\u003E\u003Cdiv\u003E\u0026nbsp;\u0026nbsp;\u0026nbsp;\u0026nbsp;  \u003Cspan style=\"color: #569cd6;\"\u003Efunction\u003C\/span\u003E \u003Cspan style=\"color: #dcdcaa;\"\u003EisLoggedIn\u003C\/span\u003E(\u003Cspan style=\"color: #9cdcfe;\"\u003Ereq\u003C\/span\u003E, \u003Cspan style=\"color: #9cdcfe;\"\u003Eres\u003C\/span\u003E, \u003Cspan style=\"color: #9cdcfe;\"\u003Enext\u003C\/span\u003E) {\u003C\/div\u003E\u003Cdiv\u003E\u0026nbsp;\u0026nbsp;\u0026nbsp;\u0026nbsp;\u0026nbsp;\u0026nbsp;\u0026nbsp;\u0026nbsp;\u003Cspan style=\"color: #c586c0;\"\u003Eif\u003C\/span\u003E (\u003Cspan style=\"color: #9cdcfe;\"\u003Ereq\u003C\/span\u003E.\u003Cspan style=\"color: #dcdcaa;\"\u003EisAuthenticated\u003C\/span\u003E()) {\u003C\/div\u003E\u003Cdiv\u003E\u0026nbsp;\u0026nbsp;\u0026nbsp;\u0026nbsp;\u0026